                                            </div>
                                            <div class="form-group">
                                                <label class="label-title">
                                                    New Password</label>
                                                <asp:TextBox ID="txt_NewPassword" TextMode="Password" class="form-control" Width="35%"
                                                    runat="server" ValidationGroup="password"></asp:TextBox><span style="color: #ff0000">
                                                    **</span>
                                                <asp:RequiredFieldValidator ID="rf_NewPassword" runat="server" ErrorMessage="New Password is Mandatory"
                                                    Display="Dynamic" ValidationGroup="password" ControlToValidate="txt_NewPassword"></asp:RequiredFieldValidator>
                                                <asp:RegularExpressionValidator ID="re_CurrentPassword" runat="server" ErrorMessage="Password must be atleast one each of on special character,number,special character and length of 7 to 20"
                                                    ValidationGroup="password" ControlToValidate="txt_NewPassword" ValidationExpression="^(?=.*[a-zA-Z])(?=.*\d)(?=.*[!@#$%^&*()_+])[A-Za-z\d][A-Za-z\d!@#$%^&*()_+]{7,19}$" />
                                            </div>
                                            <div class="form-group">
                                                <label class="label-title">
                                                    Confirm New Password</label>
                                                <asp:TextBox ID="txt_ConfirmPassword" TextMode="Password" class="form-control" Width="35%"
                                                    runat="server" ValidationGroup="password"></asp:TextBox><span style="color: #ff0000">
                                                    **</span>
                                                <asp:RequiredFieldValidator ID="rf_ConfirmPassword" runat="server" ErrorMessage="Confirm Password is Mandatory"
                                                    Display="Dynamic" ValidationGroup="password" ControlToValidate="txt_ConfirmPassword"></asp:RequiredFieldValidator>
                                                <asp:CompareValidator ID="cmp_ConfirmNewPassword" runat="server" ErrorMessage="Confirm Password must be same as New Password"
                                                    Display="Dynamic" ValidationGroup="password" ControlToCompare="txt_NewPassword"
                                                    ControlToValidate="txt_ConfirmPassword"></asp:CompareValidator>
                                            </div>
                                        </div>
