    <script type="text/javascript">
        $(".navbar-expand-toggle").click(function () {
            var div1Class = $('.navbar-expand-toggle').attr('class');
            if (div1Class == "navbar-expand-toggle") {
                if ($(window).width() >= 250 && $("#icon_Login").is(":visible") != false) {
                    $("#Side_Menu").show();
                }
            }
            else {
                if ($(window).width() <= 250) {
                    $("#Side_Menu").hide();
                }

            }
        });
        function ConfirmDelete(data) {
            var confirm_value = document.createElement("INPUT");
            confirm_value.type = "hidden";
            confirm_value.name = "confirm_value";
            if (confirm("Are you sure want to delete" + data)) {
                confirm_value.value = "Yes";
            } else {
                confirm_value.value = "No";
            }
            document.forms[0].appendChild(confirm_value);
        }
        function ConfirmHold(data) {
            var confirm_value = document.createElement("INPUT");
            confirm_value.type = "hidden";
            confirm_value.name = "confirm_value";
            if (confirm("Are you sure want to hold" + data)) {
                confirm_value.value = "Yes";
            } else {
                confirm_value.value = "No";
            }
            document.forms[0].appendChild(confirm_value);
        }
    </script>
    <script type="text/javascript">
        function no_alpha() {
            var len_vol = eval(document.form1.volume.value.length);
            var len_qr = eval(document.form1.qr.value.length);
            if (len_vol == 7) {
                return false;
            }
            if (event.keyCode >= 48 && event.keyCode <= 57 || event.keyCode == 8 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 46) {
                return true;
            }
            else {
                return false;
            }
        }

        function alphanumeric() {
            if (event.keyCode >= 97 && event.keyCode <= 122 || event.keyCode >= 65 && event.keyCode <= 90 || event.keyCode >= 48 && event.keyCode <= 57 || event.keyCode == 32 || event.keyCode == 45 || event.keyCode == 46 || event.keyCode == 44 || event.keyCode == 46 || event.keyCode == 16 || event.keyCode == 8 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 46) {

                return true;
            }
            else {
                return false;
            }
        }

        function alpha() {

            if (event.keyCode >= 97 && event.keyCode <= 122 || event.keyCode >= 65 && event.keyCode <= 90 || event.keyCode == 32 || event.keyCode == 8 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 46) {

                return true;
            }
            else {
                return false;
            }
        }

    </script>
