                                                <asp:TextBox ID="txt_CurrentPassword" TextMode="Password" class="form-control" Width="35%"
                                                    runat="server" ValidationGroup="password"></asp:TextBox><span style="color: #ff0000">
                                                    **</span>
                                                <asp:RequiredFieldValidator ID="rf_CurrentPassword" runat="server" ErrorMessage="Current Password is Mandatory"
